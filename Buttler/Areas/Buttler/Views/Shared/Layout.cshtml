<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/ButtlerStyle")

    @RenderSection("header", false)
</head>
<body>
    @Html.Partial("Nav")
    <div class="container">
        <div class="row">
            <div class="span4">
                <h1>Buttler</h1>
            </div>
        </div>

        <div class="row">
            <div class="span12">

            </div>
        </div>

        <div class="row">
            <div class="span12">
                @RenderBody()
            </div>
        </div>
    </div>
    @RenderSection("scripts", false)
    @RenderSection("footer", false)
</body>
</html>
